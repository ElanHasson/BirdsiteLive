{
  "Logging": {
    "Type": "none",
    "InstrumentationKey": "key",
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Instance": {
    "Name":  "BirdsiteLIVE",
    "Domain": "domain.name",
